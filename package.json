    "mock-fs": "4.13.0",
    "next": "^9.5.3",
    "node-sass": "4.14.1",
    "prettier": "1.19.1",
    "pretty": "2.0.0",
    "prop-types": "15.7.2",
    "radioactive-state": "1.3.4",
    "react": "^16.13.1",
    "react-bootstrap": "1.0.1",
    "react-dom": "^16.13.1",
    "react-google-maps": "9.4.5",
